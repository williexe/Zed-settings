"dependensies": {
	"prefix": "nwk-dependensies",
	"body": [
		"function tipe_verkry(vertoonAand, kontrole) {",
		"    if (vertoonAand) {",
		"      $('.' + kontrole).show();",
		"    } else {",
		"      $('.' + kontrole).hide();",
		"      $('.' + kontrole).children().find('input, select, textarea').each(function(e) {",
		"    switch ($(this).get(0).type) {",
		"        case 'select-one':",
		"          $(this).select2('val', '');",
		"        break;",
		"        case 'radio':",
		"          case 'checkbox':",
		"            $(this).attr('checked', false);",
		"        break;",
		"        case 'text':",
		"        case 'textarea':",
		"          $(this).val('');",
		"        break;",
		"       }",
		"      });",
		"    }",
		"  }",
	],
},

